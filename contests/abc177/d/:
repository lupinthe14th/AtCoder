package main

import (
	"fmt"
	"log"
)

func solution(n, m int, ab [][2]int) int {
	out := 0
	q := make([][2]int, 0)

	memo := make(map[int][]int)
	for i := range ab {
		memo[ab[i][0]] = append(memo[ab[i][0]], ab[i][1])
		memo[ab[i][1]] = append(memo[ab[i][1]], ab[i][0])
	}
	log.Print(memo)
	return out
}

func main() {
	var n, m int
	fmt.Scan(&n, &m)
	ab := make([][2]int, m)
	for i := range ab {
		fmt.Scan(&ab[i][0], &ab[i][1])
	}
	fmt.Println(solution(n, m, ab))
}
